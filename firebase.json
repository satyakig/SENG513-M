{
  "hosting": {
    "public": "lab1",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|css|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=8400"
          }
        ]
      }
    ],
    "rewrites": [],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
